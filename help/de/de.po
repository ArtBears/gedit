msgid "Ctrl + Shift + L"
msgstr "Umschalt + Strg + L"

#: C/gedit-shortcut-keys.page:75(td/p)
msgid "Close all tabs"
msgstr "Alle Reiter schließen"

#: C/gedit-shortcut-keys.page:75(td/p)
msgid "Ctrl + Shift + W"
