<?xml version="1.0"?>
<gedit:TagList xmlns:gedit="http://gedit.sourceforge.net/some-location">
  
  <gedit:TagGroup name="HTML - Tags">

    <gedit:Tag name="Abbreviated form">
      <gedit:Begin>&lt;ABBR&gt;</gedit:Begin>
      <gedit:End>&lt;/ABBR&gt;</gedit:End>
    </gedit:Tag>

    <gedit:Tag name="Abbreviation">
      <gedit:Begin>ABBR=</gedit:Begin>
    </gedit:Tag>

    <gedit:Tag name="Above">
      <gedit:Begin>&lt;ABOVE&gt;</gedit:Begin>
    </gedit:Tag>

    <gedit:Tag name="Accessibility Key Character">
      <gedit:Begin>ACCESSKEY=</gedit:Begin>
    </gedit:Tag>
  
    <gedit:Tag name="Acronym">
      <gedit:Begin>&lt;ACRONYM&gt;</gedit:Begin>
