#: plugins/diff/diff.c:62
msgid "Co_mpare Files..."
msgstr "Fájlok össze_hasonlítása..."

#: plugins/diff/diff.c:65
msgid "Makes a diff file from two documents or files"
msgstr "diff (különbség) fájlt készít 2 dokumentumból vagy fájlból"

#: plugins/diff/diff.c:67
msgid "Compare files"
msgstr "Fájlok összehasonlítása"

#. Create the dialog
#: plugins/diff/diff.c:268
msgid "Compare two files..."
msgstr "Két fájl összehasonlítása..."

#: plugins/diff/diff.c:282
msgid "C_ompare"
msgstr "Összehas_onlítás"

#: plugins/diff/diff.c:503
msgid "The two documents you selected are the same."
msgstr "A két kijelölt dokumentum megegyezik."

#: plugins/diff/diff.c:511
msgid ""
"The \"first\" file you selected does not exist.\n"
"\n"
"Please provide a valid file."
msgstr ""
"Az \"első\" kiválasztott fájl nem létezik.\n"
"\n"
"Kérlek adj meg egy érvényes fájlnevet."

#: plugins/diff/diff.c:520
msgid ""
"The \"second\" file you selected does not exist.\n"
"\n"
"Please provide a valid file."
msgstr ""
"A \"második\" kiválasztott fájl nem létezik.\n"
"\n"
"Kérlek adj meg egy érvényes fájlnevet."

#: plugins/diff/diff.c:528
msgid "The two files you selected are the same."
msgstr "A két kijelölt dokumentum megegyezik."

#: plugins/diff/diff.c:546
msgid "The \"first\" document contains no text."
msgstr "Az \"első\" dokumentumban nincs szöveg."

#: plugins/diff/diff.c:581
msgid "The \"second\" document contains no text."
msgstr "A \"második\"dokumentumban nincs szöveg."

#. FIXME: do better error reporting ... . Chema
#: plugins/diff/diff.c:607
msgid ""
"Impossible to compare the selected documents.\n"
"\n"
"gedit could not create a temporary file."
msgstr ""
"Lehetetlen a kiválasztott dokumentumok összehasonlítása.\n"
"\n"
"A gedit nem tudott ideiglenes fájlt létrehozni."

#: plugins/diff/diff.c:625 plugins/diff/diff.c:638
msgid ""
"Impossible to compare the selected documents.\n"
"\n"
"Error executing the diff command."
msgstr ""
"Lehetetlen a kiválasztott dokumentumok összehasonlítása.\n"
"\n"
"Hiba történt a diff parancs futtatása közben."

#: plugins/diff/diff.c:660
msgid "No differences were found between the selected documents."
msgstr "Nincs különbség a kiválasztott dokumentumok között."

#: plugins/diff/diff.c:701
msgid ""
"Impossible to compare the selected documents.\n"
"\n"
"The result contains invalid UTF-8 data."
msgstr ""
"Lehetetlen összehasonlítani a kiválasztott dokumentumokat.\n"
"\n"
"Az eredmény érvénytelen UTF-8 adatokat tartalmaz."

#: plugins/diff/diff.c:925
msgid ""
"Makes a diff file from two documents or files on disk.\n"
"\n"
"For more info on \"diff\" program, type \"man diff\" in a shell prompt.\n"
msgstr ""
"diff (különbség) fájlt készít 2 dokumentumból vagy fájlból.\n"
"\n"
"A diff parancsról megtudhatsz többet, ha a shell promptnál beírod, hogy "
"\"man diff\".\n"

#: plugins/diff/diff.c:928
msgid ""
"Copyright (C) 2000 - 2001 Chema Celorio \n"
"Copyright (C) 2002 Paolo Maggi"
