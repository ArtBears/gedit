msgstr "Vytvoří soubor s rozdíly \"diff\" ze dvou dokumentů nebo souborů"

#. Create the dialog
#: plugins/diff/diff.c:271 plugins/diff/diff.gedit-plugin.desktop.in.h:1
msgid "Compare Files"
msgstr "Porovnat soubory"

#: plugins/diff/diff.c:285
msgid "C_ompare"
msgstr "P_orovnat"

#: plugins/diff/diff.c:513
msgid "Executed command"
msgstr "Prováděný příkaz"

#: plugins/diff/diff.c:654
msgid "The two documents you selected are the same."
msgstr "Dokumenty, které jste vybral, jsou stejné."

#: plugins/diff/diff.c:662
msgid ""
"The \"first\" file you selected does not exist.\n"
"\n"
"Please provide a valid file."
msgstr ""
"První soubor, který jste vybral, neexistuje.\n"
"\n"
"Prosím vyberte platný soubor."

#: plugins/diff/diff.c:671
msgid ""
"The \"second\" file you selected does not exist.\n"
"\n"
"Please provide a valid file."
msgstr ""
"Druhý soubor, který jste vybral, neexistuje.\n"
"\n"
"Prosím vyberte platný soubor."

#: plugins/diff/diff.c:679
msgid "The two files you selected are the same."
msgstr "Soubory, které jste vybral, jsou stejné."

#: plugins/diff/diff.c:696
msgid "The \"first\" document contains no text."
msgstr "První dokument neobsahuje žádný text."

#: plugins/diff/diff.c:730
msgid "The \"second\" document contains no text."
msgstr "Druhý dokument neobsahuje žádný text."

#. FIXME: do better error reporting ... . Chema
#: plugins/diff/diff.c:756
msgid ""
"Impossible to compare the selected documents.\n"
"\n"
"gedit could not create a temporary file."
msgstr ""
"Nelze porovnat vybrané dokumenty.\n"
"\n"
"Nebylo možné vytvořit dočasný soubor."

#: plugins/diff/diff.c:780 plugins/diff/diff.c:793
msgid ""
"Impossible to compare the selected documents.\n"
"\n"
"Error executing the diff command."
msgstr ""
"Nelze porovnat vybrané dokumenty.\n"
"\n"
"Chyba při spuštění příkazu diff."

#: plugins/diff/diff.c:815
msgid "No differences were found between the selected documents."
msgstr "Mezi vybranými dokumenty nebyly nalezeny žádné rozdíly."

#: plugins/diff/diff.c:856
msgid ""
"Impossible to compare the selected documents.\n"
